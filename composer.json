{
  "require": {
    "carlcs/craft-redactorcustomstyles": "3.0.4",
    "chasegiunta/scss": "1.0.0",
    "cloudgrayau/pitch": "1.2.1",
    "craftcms/ckeditor": "1.3.0",
    "craftcms/cms": "3.7.61",
    "craftcms/redactor": "2.10.10",
    "doublesecretagency/craft-cpcss": "2.4.0",
    "focuslabllc/craft-cheat-sheet": "2.0.3",
    "guilty/craft-rss": "1.0.1",
    "mmikkel/cp-clearcache": "1.3.0",
    "nystudio107/craft-code-field": "3.0.6",
    "nystudio107/craft-templatecomments": "1.2.0",
    "nystudio107/craft-youtubeliveembed": "1.0.9",
    "putyourlightson/craft-sprig": "1.13.0",
    "solspace/craft-calendar": "3.3.17",
    "spicyweb/craft-embedded-assets": "2.11.3",
    "studioespresso/craft-date-range": "2.5.0",
    "svenjungnickel/redactor-font-awesome": "2.1.1",
    "unionco/calendarize": "1.2.18",
    "utakka/redactor-anchors": "1.4.0",
    "verbb/calendar-links": "2.0.2",
    "verbb/cp-nav": "3.0.17",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.5"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
